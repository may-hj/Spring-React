package com.example.demo.bbs.domain;

import javax.persistence.Entity;
import javax.persistence.*;

@Entity
@Table(name="board")
public class Board {
	
	@Column(name="board_no")
	private int boardNo;
	
	@Column(name="regdate")
	private int regdate;
	
	@Column(name="username")
	private String username;
	
	@Column(name="userid")
	private String userid;
	
	@Column(name="passwd")
	private String passwd;
}
